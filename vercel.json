{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/client/assets/$1"
    },
    {
      "source": "/_expo/(.*)",
      "destination": "/client/_expo/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/index.html"
    }
  ]
}